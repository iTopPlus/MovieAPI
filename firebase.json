{
    "database": {
      "rules": "database.rules.json"
    },
    "hosting": {
      "public": "/index.html",
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ],
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    }
  }
  